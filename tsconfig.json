{
	"compilerOptions": {
		"allowJs": true,
		"checkJs": false,
		"target": "es6",
		"module": "esnext",
		"useDefineForClassFields": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"outDir": "assets/js",
		"lib": ["ES2021", "DOM", "DOM.Iterable"]
	},
	"include": ["source/js/**/*", "global.d.ts"]
}
