{
  "name": "iframe-test-site",
  "version": "1.0.0",
  "description": "",
  "source": "src/parent.html",
  "type": "module",
  "scripts": {
    "test": "jest --coverage --maxWorkers=50%",
    "build": "npx tsc",
    "format": "npx biome format ./source",
    "format:write": "npx biome format ./source --write",
    "lint": "npx biome lint ./source",
    "lint:write": "npx biome lint ./source --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/helsingborg-stad/iframe-test-site.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/helsingborg-stad/iframe-test-site/issues"
  },
  "homepage": "https://github.com/helsingborg-stad/iframe-test-site#readme",
  "devDependencies": {
    "@biomejs/biome": "2.3.5",
    "@types/jest": "^30.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "ts-jest": "^29.4.5",
    "typescript": "^5.9.2"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(js|ts)$": "ts-jest"
    },
    "collectCoverageFrom": [
      "source/**/*.js",
      "source/**/*.ts"
    ],
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "testMatch": [
      "**/*.test.(js|ts)"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(.*\\.mjs$))"
    ]
  }
}